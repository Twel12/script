<?xml version="1.0" encoding="UTF-8"?>
<manifest>
<remote name="Twel12" fetch="https://github.com/Twel12/" />
<remote name="REPO" fetch="https://github.com/PixelOS-and-Not-So-Pixel" />

<!-- Track Stuff from our repo -->
<remove-project name="frameworks_base" />
<project path="frameworks/base" name="frameworks_base" remote="REPO" revision="eleven" />
<remove-project name="packages_apps_Settings" />
<project path="packages/apps/Settings" name="packages_apps_Settings" remote="REPO" revision="eleven" />
<remove-project name="packages_apps_Updates" />
<project path="packages/apps/Updates" name="packages_apps_Updates" remote="REPO" revision="eleven" />

<!-- kernel -->
<project path="kernel/xiaomi/sm6150" name="android_kernel_xiaomi_sm6150" remote="Twel12" revision="Bantom" />

<!-- Xiaomi vendor -->
<project path="vendor/xiaomi/davinci" name="proprietary_vendor_xiaomi_davinci" remote="Twel12" revision="eleven" />

<!-- Track Hardware Stuff from los -->
<project path="hardware/xiaomi" name="android_hardware_xiaomi" remote="Twel12" revision="lineage-18.0" />

<!-- PE patches for davinci -->
<project path="patches" name="build_patches" remote="REPO" revision="eleven" />
  
<!-- davinci device tree -->
<project path="device/xiaomi/davinci" name="device-xiaomi-davinci" remote="Twel12" revision="eleven" />

<!--Track Hals From My Repo-->
<remove-project name="hardware_qcom-caf_sm8150_audio" />
<project name="hardware-qcom-caf-sm8150-audio" path="hardware/qcom-caf/sm8150/audio" remote="Twel12" revision="lineage-18.0-caf-sm8150" />
<remove-project name="hardware_qcom-caf_sm8150_display" />
<project name="hardware-qcom-caf-sm8150-display" path="hardware/qcom-caf/sm8150/display" remote="Twel12" revision="lineage-18.0-caf-sm8150" />
<remove-project name="hardware_qcom-caf_sm8150_media" />
<project name="hardware-qcom-caf-sm8150-media" path="hardware/qcom-caf/sm8150/media" remote="Twel12" revision="master" />

</manifest>
